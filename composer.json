{
    "name": "jordanbrauer/tally-ticker",
    "description": "Count your iterations cleanly.",
    "type": "component",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordan Brauer",
            "email": "jbrauer.inc@gmail.com"
        }
    ],
    "scripts": {
        "test": "phpunit --configuration=./phpunit.xml --testsuite=fullspec --color=always",
        "clover": "phpunit --coverage-clover=./clover.xml --configuration=./phpunit.xml --testsuite=fullspec --color=never"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "symfony/var-dumper": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "TallyTicker\\": "src/TallyTicker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TallyTicker\\Tests\\": "tests/TallyTicker/"
        }
    }
}
